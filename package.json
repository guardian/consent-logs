{
    "name": "Consent logs",
    "version": "0.0.1",
    "author": "The Guardian <commercial.dev@theguardian.com>",
    "description": "Track user consents and store an audit of those events in the Data
Lake.",
    "isAwsLambda": true,
    "cloudformation": false,
    "projectName": "Frontend::Consent Logs",
    "buildDir": "./dist",
    "riffraffFile": "./riff-raff.yaml",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "tsc",
        "local": "node dist/local.js",
        "package": "ARTEFACT_PATH=$PWD/dist VERBOSE=true riffraff-artefact"
    },
    "devDependencies": {
        "@types/aws4": "^1.5.1",
        "@types/aws-lambda": "^8.10.24",
        "@types/express": "^4.14.0",
        "@types/node": "^11.13.7",
        "@types/node-fetch": "^2.3.2",
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "node-riffraff-artefact": "^2.0.1",
        "typescript": "^3.4.0"
    },
    "dependencies": {
        "aws-sdk": "^2.441.0",
        "aws4": "^1.8.0",
        "node-fetch": "^2.3.0"
    }
}
